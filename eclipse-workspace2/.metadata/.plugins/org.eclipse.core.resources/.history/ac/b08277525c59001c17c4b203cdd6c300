<mapper namespace="com.khh.mapper.memberMapper">

    <insert id="insertMember">
        insert into member(id , age, salary) values ( #{id}, #{age}, #{salary})
    </insert>
	
	<delete id="deleteMember">
	
	delete from member where id = #{id}
	
	
	</delete>
	
	
	<update id="updateMember">
	
	update member set id=#{id}, age=#{age}, salary=#{salray}
	
	
	</update>
	
</mapper>
